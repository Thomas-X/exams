{
  "name": "razzle-examples-with-typescript",
  "version": "0.8.12",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@types/lodash": "^4.14.104",
    "@types/react-bootstrap": "^0.32.6",
    "@types/react-select": "^1.2.4",
    "@types/redux-persist": "^4.3.1",
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.3",
    "cookies": "^0.7.1",
    "cookies-js": "^1.2.3",
    "express": "^4.15.3",
    "lodash": "^4.17.5",
    "react": "^16.0.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.0.0",
    "react-grid-system": "^3.1.2",
    "react-obfuscate": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-spinners": "^0.2.6",
    "react-table": "^6.8.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-persist": "^5.9.1",
    "redux-persist-cookie-storage": "^1.0.0-beta",
    "redux-thunk": "^2.2.0",
    "remove": "^0.1.5",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/history": "^4.6.2",
    "@types/jest": "^20.0.1",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.0",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "@types/redux-form": "^7.0.14",
    "@types/serialize-javascript": "^1.3.2",
    "@types/webpack-env": "^1.13.2",
    "raf": "^3.4.0",
    "razzle": "^0.8.12",
    "ts-jest": "20.0.6",
    "ts-loader": "2.3.4",
    "tslint": "5.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "2.7.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
